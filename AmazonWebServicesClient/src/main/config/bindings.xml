<bindings xmlns="http://java.sun.com/xml/ns/jaxws">
    <enableAsyncMapping>true</enableAsyncMapping>
    <enableWrapperStyle>false</enableWrapperStyle>

    <handler-chains xmlns="http://java.sun.com/xml/ns/javaee">
        <handler-chain>
            <handler>
                <handler-name>AmazonSecurityHandler</handler-name>
                <handler-class>org.amazon.util.AmazonSecurityHandler</handler-class>
            </handler>
            <handler>
                <handler-name>LoggingHandler2</handler-name>
                <handler-class>org.amazon.util.LoggingHandler</handler-class>
            </handler>
        
        </handler-chain>
    </handler-chains>
</bindings>